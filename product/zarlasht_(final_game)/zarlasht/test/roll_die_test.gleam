pub fn random_anim_dice_test() {
  let dice =
    anim_get_next_dice()
    |> list.repeat(10)

  dice
  |> list.each(fn(die) {
    dice
    |> list.find(fn(die2) { die != die2 })
    |> should.be_ok()
  })
}

